# Supabase Configuration
# Copie este arquivo para .env.local para desenvolvimento local
# ou para .env para produção

# URL do seu projeto Supabase
VITE_SUPABASE_URL=your_supabase_url_here

# Chave pública (anon key) do Supabase
# Esta chave é segura para ser usada no frontend
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Service Role Key (para scripts de setup)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Admin User para desenvolvimento
ADMIN_EMAIL=admin@yourcompany.com
ADMIN_PASSWORD=secure_password
ADMIN_NAME=Admin User

# Configurações de Ambiente
NODE_ENV=development
VITE_APP_ENV=development

# URLs de Desenvolvimento
VITE_API_URL=http://localhost:3000
VITE_APP_URL=http://localhost:5173
